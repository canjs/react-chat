{
  "name": "chat",
  "version": "1.0.0",
  "main": "chat.js",
  "scripts": {
    "eslint": "eslint \"**/*.js\" --ignore-pattern \"dist\" --ignore-pattern \"node_modules\"",
    "develop": "done-serve --develop --static --port 8080",
    "build": "npm run eslint && steal-tools build"
  },
  "dependencies": {
    "can-define": "^2.5.10",
    "can-fixture": "^3.0.0",
    "can-realtime-rest-model": "^1.0.1",
    "can-route": "^4.3.12",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "steal": "^1.12.5",
    "ylem": "^2.1.0"
  },
  "devDependencies": {
    "done-serve": "^2.3.0",
    "steal-tools": "^2.0.5"
  },
  "steal": {
    "plugins": [
      "steal-css"
    ],
    "babelOptions": {
      "plugins": [
        "transform-class-properties",
        "transform-decorators-legacy"
      ]
    }
  }
}
